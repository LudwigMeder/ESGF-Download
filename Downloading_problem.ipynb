    "##### Importing required libraries #####\n",
    "import os\n",
    "import requests\n",
    "import xarray as xr\n",
    "import requests_cache\n",
    "from pyesgf.search import SearchConnection\n",
    "from pyesgf.search.results import FileResult\n",
    "requests_cache.core.CachedSession\n",
    "\n",
    "##### Data set characteristics ####\n",
    "mlevel=[2,3]  # slicing vertical levels\n",
    "vars=[\"ua\",\"va\",\"ta\",\"hus\"] \n",
    "## slicing Europe\n",
    "lon1=-25     # lon_west \n",
    "lon2= 65     # lon_east\n",
    "lat1= 35     # lat_south\n",
    "lat2= 72     # lat_north\n",
    " \n",
    "model=\"HadGEM3-GC31-MM\"\n",
    "#model=\"CMCC-CM2-SR5\"\n",
    "lista_modelos=[model] # line is required for the possibility to add more models\n",
    "experiment_id=\"historical\"\n",
    "variant_label=\"r1i1p1f3\"\n",
    "#variant_label=\"r1i1p1f1\"\n",
    "frequency=\"6hrPt\"\n",
    "table_id=\"6hrLev\"\n",
    "\n",
    "dpath=\"data/\" # required for saving the downloaded data\n",
    "##### Search for data set #####\n",
    "conn = SearchConnection('http://esgf-data.dkrz.de/esg-search', distrib=True)\n",
    "# set output path\n",
    "opath = dpath+ \\\n",
    "        \"CMIP6\"+\"/\"+ \\\n",
    "        model+\"/\"+ \\\n",
    "        experiment_id+\"/\"+\\\n",
    "        variant_label+\"/\"      \n",
    "if not os.path.exists(opath): # prove if the file already exists\n",
    "    os.makedirs(opath)\n",
    "    \n",
    "for var in vars:\n",
    "    print(\"Work on Variable: \",var)\n",
    "    ctx = conn.new_context(project='CMIP6',\n",
    "    experiment_id=experiment_id,\n",
    "    variable=var,\n",
    "    frequency=frequency,\n",
    "    variant_label=variant_label,\n",
    "    table_id=table_id,\n",
    "    source_id= lista_modelos\n",
    "    )\n",
    "    ctx.hit_count\n",
    "    ctx.facet_counts['source_id'].keys()\n",
    "# Obtain urls lists\n",
    "    lista_urls=[]\n",
    "    for i in range(len(ctx.search())):\n",
    "        try:\n",
    "            result = ctx.search()[i]\n",
    "            print(result.dataset_id,'..............ok')\n",
    "            lista_urls.append(result)\n",
    "        except:\n",
    "            pass\n",
    "            print('-------------------------------')\n",
    "            print('Lista finales')\n",
    "            lista_urls\n",
    "    \n",
    "    tr=[x for x in lista_urls if lista_modelos[0] in x.dataset_id]\n",
    "    files = tr[0].file_context().search()\n",
    "    lista = [i.opendap_url for i in files]\n",
    "\n",
    "\n",
    "    # check, if the strings in lista are already downloaded\n",
    "    # if yes, url is deleted from lista\n",
    "    onlyfiles = [f for f in os.listdir(opath) if os.path.isfile(os.path.join(opath, f))]\n",
    "    onlyfiles.sort()\n",
    "\n",
    "    for f,string in enumerate(onlyfiles):\n",
    "        string=onlyfiles[f].split('_')\n",
    "        string=string[0]+\"_\"+table_id+\\\n",
    "               \"_\"+string[2]+\\\n",
    "               \"_\"+string[3]+\"_\"+string[4]+\\\n",
    "               \"_\"+\"gn\"+\\\n",
    "               \"_\"+string[-2]+\"-\"+string[-1]\n",
    "\n",
    "        for i in lista:\n",
    "            if i.__contains__(string):\n",
    "                lista.remove(i)\n",
    "                    \n",
    "     #Download Data\n",
    "    for d,x_url in enumerate(lista):\n",
    "        ds = xr.open_dataset(x_url)\n",
    "\n",
    "        data=ds.isel(lev=mlevel)\n",
    "        data=data.sel(bnds=1)\n",
    "\n",
    "        data_new=data.where((ds.lon>=lon1) & (ds.lon<=lon2) &(ds.lat>=lat1) & (ds.lat<=lat2),drop=True)\n",
    "\n",
    "        nfileYear=(lista[d].split('/')[-1].split(\"_\")[-1]).replace(\"-\", \"_\")\n",
    "        file_name=var+\"_\"+\\\n",
    "                    \"CMIP6\"+\"_\"+\\\n",
    "                    model+\"_\"+\\\n",
    "                    experiment_id+\"_\"+\\\n",
    "                    variant_label+\"_\"+\\\n",
    "                    nfileYear     \n",
    "\n",
    "        data_new.to_netcdf(opath+file_name)\n",
    "        print(\"Done with Years: \",nfileYear)\n",
    "    print(\"Done with Variable: \",var)\n",
    "            "
   ]
